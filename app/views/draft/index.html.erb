<h1>Drafts</h1>

<%= link_to "New Drafts", new_article_path %>

<ul>
  <% @articles.each do |article| %>
  	<% if current_user.id == article.user_id %>
      	<li>
      		<a href="<%= article_path(article) %>/edit">
        	<%= article.title %>
      	</li>
    <% end %>
  <% end %>
</ul>
<p><%= link_to 'Go to my articles', articles_path %></p>
<a href="/">Home page</a>